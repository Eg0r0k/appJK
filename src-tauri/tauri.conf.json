{
  
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "jk",
  "version": "0.1.0",
  "identifier": "com.jk.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  
  "app": {
    
    "windows": [
      {
        "contentProtected": false,
        "title": "jk",
        "width": 800,
        "height": 600,
        "minHeight": 400,
        "minWidth": 550
      }
    ],
  
    "security": {
      "csp": null
    }
    
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
